<!DOCTYPE html>
<html lang='en'>
<head>
  <meta charset='UTF-8' />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Universal Redux Boilerplate</title>
  <%# Stylesheets from `webpack-stats.json` %>
  <% assets.style.forEach(function (style) { %>
    <link rel='stylesheet' href='<%= style %>' />
  <% }); %>
</head>
<body>
  <%# React App output %>
  <div id='content'><%- body %></div>

  <%# JavaScript from `webpack-stats.json` %>
  <% assets.script.forEach(function (script) { %>
    <script async defer src="<%= script %>"></script>
  <% }); %>
</body>
</html>
